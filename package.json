{
  "name": "@ptomulik/octokit-paginate-rest-postproc",
  "version": "0.0.0-development",
  "description": "Postprocessor for arrays returned by @octokit/plugin-paginate-rest",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ts": "tsd",
    "pretest:ts": "npm run build",
    "build": "pika build",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint --fix '**/*.ts'",
    "pre-checkin": "npm run lint && npm run build && npm run test:coverage && npm run test:ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptomulik/octokit-paginate-rest-postproc"
  },
  "keywords": [],
  "author": "Pawe≈Ç Tomulik <ptomulik@meil.pw.edu.pl>",
  "license": "MIT",
  "devDependencies": {
    "@octokit/graphql": "^4.6.2",
    "@octokit/plugin-paginate-rest": "^2.13.3",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "semantic-release": "^17.4.3",
    "semantic-release-plugin-update-version-in-files": "^1.1.0",
    "ts-jest": "^26.5.6",
    "tsd": "^0.15.1",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./pkg"
        }
      ],
      [
        "semantic-release-plugin-update-version-in-files",
        {
          "files": [
            "pkg/dist-web/*",
            "pkg/dist-node/*",
            "pkg/*/version.*"
          ]
        }
      ]
    ]
  },
  "renovate": {
    "extends": [
      "github>octokit/.github"
    ]
  },
  "source": "pkg/dist-src/index.js",
  "types": "pkg/dist-types/index.d.ts",
  "main": "pkg/dist-node/index.js",
  "module": "pkg/dist-web/index.js"
}
